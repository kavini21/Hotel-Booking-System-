# Copy this file to .env and fill values before running the server
# MongoDB (required)
MONGODB_URI=mongodb://localhost:27017

# Cloudinary (optional, required if you use uploads)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Clerk (server-side)
CLERK_SECRET_KEY=sk_your_secret_key
CLERK_PUBLISHABLE_KEY=pk_your_publishable_key
CLERK_SIGN_IN_URL=http://localhost:5175

# App
PORT=3000
